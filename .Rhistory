steps.a.day <- aggregate(steps ~ date, data = activity,
FUN = function(x) sum=sum(x), na.rm = TRUE)
steps.a.day$steps1000 <- round(steps.a.day$steps/1000, digits = 2)
steps.a.day1 <- select(steps.a.day, date, steps1000)
